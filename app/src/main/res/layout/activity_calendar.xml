<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:gravity="center"
    android:orientation="vertical">


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="71dp"
        android:layout_height="63dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="2dp"
        android:layout_marginTop="0dp"
        app:srcCompat="@drawable/bap" />

    <CalendarView
        android:id="@+id/calendarView"
        android:layout_width="417dp"
        android:layout_height="323dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="-5dp"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="1dp"
        android:gravity="top" />

    <TextView
        android:id="@+id/Date"
        android:layout_width="416dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/calendarView"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="3dp"
        android:layout_marginTop="-392dp"
        android:layout_marginEnd="-8dp"
        android:gravity="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Large" />

    <EditText
        android:id="@+id/Break"
        android:layout_width="408dp"
        android:layout_height="42dp"
        android:layout_below="@id/Date"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="3dp"
        android:layout_marginTop="371dp"
        android:layout_marginEnd="2dp"
        android:ems="10"
        android:gravity="center"
        android:hint="아침은 무엇을 먹었나요?"
        android:fontFamily="@font/imcresoojin"
        android:inputType="textMultiLine"
        android:textColor="@color/black"
        android:textSize="15dp"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/Outputb"
        android:layout_width="408dp"
        android:layout_height="42dp"
        android:layout_below="@id/Date"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="3dp"
        android:layout_marginTop="371dp"
        android:layout_marginEnd="0dp"
        android:gravity="center"
        android:textColor="@color/black"
        android:textSize="15dp"
        android:visibility="invisible" />

    <EditText
        android:id="@+id/Lunch"
        android:layout_width="408dp"
        android:layout_height="42dp"
        android:layout_below="@id/Date"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="2dp"
        android:layout_marginTop="415dp"
        android:layout_marginEnd="1dp"
        android:ems="10"
        android:gravity="center"
        android:hint="점심은 무엇을 먹었나요?"
        android:fontFamily="@font/imcresoojin"
        android:inputType="textMultiLine"
        android:textColor="@color/black"
        android:textSize="15dp"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/Outputl"
        android:layout_width="408dp"
        android:layout_height="42dp"
        android:layout_below="@id/Date"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="-2dp"
        android:layout_marginTop="416dp"
        android:layout_marginEnd="5dp"
        android:gravity="center"
        android:textColor="@color/black"
        android:textSize="15dp"
        android:visibility="invisible" />

    <EditText
        android:id="@+id/Dinner"
        android:layout_width="408dp"
        android:layout_height="42dp"
        android:layout_below="@id/Date"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="456dp"
        android:layout_marginEnd="3dp"
        android:ems="10"
        android:gravity="center"
        android:hint="저녁은 무엇을 먹었나요?"
        android:fontFamily="@font/imcresoojin"
        android:inputType="textMultiLine"
        android:textColor="@color/black"
        android:textSize="15dp"
        android:visibility="invisible" />

    <TextView
        android:id="@+id/Outputd"
        android:layout_width="408dp"
        android:layout_height="42dp"
        android:layout_below="@id/Date"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="2dp"
        android:layout_marginTop="455dp"
        android:layout_marginEnd="1dp"
        android:gravity="center"
        android:textColor="@color/black"
        android:textSize="15dp"
        android:visibility="invisible" />

    <Button
        android:id="@+id/save_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="-3dp"
        android:layout_marginEnd="-1dp"
        android:layout_marginBottom="9dp"
        android:text="식단 저장하기"
        android:fontFamily="@font/imcresoojin"
        tools:visibility="invisible" />

    <Button
        android:id="@+id/change_btn"
        android:layout_width="208dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="-1dp"
        android:layout_marginBottom="9dp"
        android:text="수정하기"
        android:fontFamily="@font/imcresoojin"
        android:visibility="invisible" />

    <Button
        android:id="@+id/delete_btn"
        android:layout_width="204dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="208dp"
        android:layout_marginEnd="-2dp"
        android:layout_marginBottom="10dp"
        android:text="삭제하기"
        android:visibility="invisible" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="71dp"
        android:layout_height="63dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="-1dp"
        app:srcCompat="@drawable/tool" />


</RelativeLayout>


