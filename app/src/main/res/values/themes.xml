<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Myapp01" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#c56824</item>
        <item name="colorPrimaryVariant">#c56824</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#A09f57</item>
        <item name="colorSecondaryVariant">#A09f57</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <item name="actionBarSize">40dp</item>
        <item name="actionBarTabTextStyle">@font/imcresoojin</item>

        <!-- Customize your theme here. -->
    </style>
    <style name="imcreTextView" parent="android:Widget.TextView">
        <item name="android:fontFamily">@font/imcresoojin</item>
    </style>

    <style name="imcreButton" parent="android:Widget.Button">
        <item name="android:fontFamily">@font/imcresoojin</item>
    </style>
</resources>